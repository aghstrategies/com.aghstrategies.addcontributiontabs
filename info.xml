<?xml version="1.0"?>
<extension key="com.aghstrategies.addcontributiontabs" type="module">
  <file>addcontributiontabs</file>
  <name>Display Related Contributions</name>
  <description>Adds contributions of households and employees to individuals' and employers' contribution tabs</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>AGH Strategies</author>
    <email>info@aghstrategies.com</email>
  </maintainer>
  <releaseDate>2015-10-22</releaseDate>
  <version>1.1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <urls>
    <url desc="Main Extension Page">https://civicrm.org/extensions/display-related-contributions</url>
    <url desc="Documentation">https://github.com/aghstrategies/com.aghstrategies.addcontributiontabs</url>
    <url desc="Support">https://aghstrategies.com/contact</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <civix>
    <namespace>CRM/Addcontributiontabs</namespace>
    <format>24.09.1</format>
  </civix>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <mixins>
    <mixin>entity-types-php@2.0.0</mixin>
  </mixins>
</extension>
